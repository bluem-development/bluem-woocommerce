{
    "name": "daanrijpkema/bluem-woocommerce",
    "license": "GPL-2.0-or-later",
    "description": "A robust code template for creating a standards-compliant WordPress plugin.",
    "keywords": [
        "wordpress", "wp", "plugin template"
    ],
    "support": {
        "issues": "https://github.com/daanrijpkema/bluem-woocommerce"
    },
    "require": {
        "nesbot/carbon": "^2.53",
        "daanrijpkema/bluem-php": "^2.0.13",
        "symfony/polyfill-php72": "^1.22",
        "symfony/polyfill-mbstring": "v1.19.0",
        "symfony/polyfill-php80": "^1.23",
        "erusev/parsedown": "^1.7",
        "ext-json": "^8.0"
    },
    "replace": {
        "symfony/polyfill-php80": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "yoast/phpunit-polyfills": "^1.0"
    }
}
